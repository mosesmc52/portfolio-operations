SHELL=/bin/bash
PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin

30 2 * * * root \
  FUND_ID=${FUND_ID:-1} \
  DAYS=${ALPACA_SYNC_DAYS:-7} \
  LIMIT=${ALPACA_SYNC_LIMIT:-500} \
  /app/scripts/manage_wrapper.sh sync_alpaca_filled_orders_last_days \
    --fund-id=$FUND_ID \
    --days=$DAYS \
    --limit=$LIMIT \
  >> /proc/1/fd/1 2>> /proc/1/fd/2
